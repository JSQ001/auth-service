<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hand.hcf.app.base.persistence.CompanyMapper">

    <select id="findTenantIdByCompanyOID" resultType="java.lang.Long">
        select c.tenant_id from art_company c where c.company_oid=#{companyOID}
    </select>

</mapper>
